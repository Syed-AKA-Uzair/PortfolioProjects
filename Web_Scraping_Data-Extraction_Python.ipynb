  {
   "cell_type": "code",
   "execution_count": 36,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Importing Libraries\n",
    "\n",
    "import pandas as pd\n",
    "from selenium import webdriver # Selenium Webdriver\n",
    "from bs4 import BeautifulSoup # Beautiful Soup\n",
    "from time import sleep\n",
    "from random import randint"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "PATH = \"C:\\Program Files (x86)\\chromedriver.exe\" # Web Driver Path\n",
    "\n",
    "# Declaring Lists to save data extracted\n",
    "product = []\n",
    "brand = []\n",
    "rating = []\n",
    "price = []\n",
    "\n",
    "\n",
    "for page_number in range(1,6): # loop for getting each Page (from 1-5)\n",
    "    # Page_url changes for each page\n",
    "    page_url = f\"https://www.amazon.sg/s?k=running+shoes&i=fashion&bbn=6315239051&rh=n%3A6315239051%2Cp_89%3AASICS%7CAdidas%7CNIKE%7CNew+Balance%7CPUMA%7CReebok%7CSkechers%2Cp_72%3A6469122051%2Cp_36%3A4000-100000&page={page_number}&qid=1653302814&ref=sr_pg_{page_number}\"\n",
    "    driver = webdriver.Chrome(PATH)\n",
    "    driver.get(page_url) # Load page using selenium driver\n",
    "    html = driver.page_source\n",
    "    \n",
    "    soup = BeautifulSoup(html, 'lxml') # Parser using BS4\n",
    "    cards = soup.find_all('div', {'data-component-type':'s-search-result'}) # Find all products listed\n",
    "    sleep(randint(2,9))\n",
    "    \n",
    "    for store in cards: # Extract item-detail from each product\n",
    "        name = store.find('span', class_ = 'a-size-base-plus a-color-base a-text-normal').text # Find the name\n",
    "        product.append(name) # Append in the list\n",
    "\n",
    "        brand_name = store.find('span', class_ = 'a-size-base-plus a-color-base').text\n",
    "        brand.append(brand_name)\n",
    "        \n",
    "        cost = store.find('span', class_ = 'a-offscreen').text.strip() if store.find('span', class_ = 'a-offscreen') else ''\n",
    "        price.append(cost)\n",
    "        \n",
    "        rate = store.find('span', {'class':'a-size-base s-light-weight-text'}).text.strip() if store.find('span', {'class':'a-size-base s-light-weight-text'}) else '' \n",
    "        rating.append(rate)\n",
    "        # repeat the process of finding and appending the list until last product in the page\n",
    "    # Moving out to next page and repeating the process until the last page of the given url\n",
    "\n",
    "# Build a data frame by combining all lists\n",
    "list_frame = pd.DataFrame({\"Product\": product, \"Brand\": brand, \"Price\": price, \"RatedBy\": rating})\n",
    "\n",
    "driver.quit() # Exit driver\n"
   ]
  },
